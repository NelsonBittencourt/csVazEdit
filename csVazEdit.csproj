<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <Version>0.002</Version>
    <Authors>Nelson Rossi Bittencourt</Authors>
    <Company>NRB Consultoria</Company>
    <Product>csVazEdit</Product>
    <Description>
    O csVazEdit é um aplicativo para converter arquivos de vazões binárias em formato texto e vice-versa.
    Além de reproduzir as funções do aplicativo original 'VazEdit' do ONS, o csVazEdit também poderá estendê-las.
    </Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="EPPlus" Version="5.6.4" />
  </ItemGroup>  

</Project>
